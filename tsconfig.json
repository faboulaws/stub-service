{
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "test"
  ],
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "declaration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "rootDir": "src",
    "strict": true,
    "skipLibCheck": true
  }
}